<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:background="?android:attr/selectableItemBackground"
    android:clickable="true"
    android:focusable="true"
    android:orientation="vertical"
    android:paddingBottom="@dimen/row_padding_vertical">

    <TextView
        android:id="@+id/service"
        android:textAlignment="center"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@color/colorPrimary"
        android:text="1" />

    <TextView
        android:id="@+id/title"
        android:layout_width="365dp"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:textAlignment="center"
        android:background="@color/colorPrimary"
        android:layout_toEndOf="@+id/service"
        android:text="Service" />

    <TextView
        android:id="@+id/availabilityTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Availability" />

    <TextView
        android:id="@+id/costTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/availabilityTitle"
        android:layout_marginStart="2dp"
        android:layout_marginTop="50dp"
        android:text="Cost" />

    <TextView
        android:id="@+id/frequencyTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/costTitle"
        android:layout_marginStart="2dp"
        android:layout_marginTop="50dp"
        android:text="Frequency" />

    <TextView
        android:id="@+id/reputationTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_toEndOf="@+id/frequencyTitle"
        android:layout_marginTop="50dp"
        android:text="Reputation" />

    <TextView
        android:id="@+id/responseTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_toEndOf="@+id/reputationTitle"
        android:layout_marginTop="50dp"
        android:text="Response" />

    <TextView
        android:id="@+id/successTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:layout_toEndOf="@+id/responseTitle"
        android:layout_marginTop="50dp"
        android:text="Success" />

    <TextView
        android:id="@+id/availability"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="70dp"
        android:textAlignment="center"
        android:text="9" />

    <TextView
        android:id="@+id/cost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/availability"
        android:layout_marginStart="35dp"
        android:layout_marginTop="70dp"
        android:text="9" />

    <TextView
        android:id="@+id/frequency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/cost"
        android:layout_marginStart="35dp"
        android:layout_marginTop="70dp"
        android:text="8" />

    <TextView
        android:id="@+id/reputation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/frequency"
        android:layout_marginStart="40dp"
        android:layout_marginTop="70dp"
        android:text="7" />

    <TextView
        android:id="@+id/response"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/reputation"
        android:layout_marginStart="45dp"
        android:layout_marginTop="70dp"
        android:text="8" />

    <TextView
        android:id="@+id/success_rate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/response"
        android:layout_marginStart="45dp"
        android:layout_marginTop="70dp"
        android:text="9" />

    <TextView
        android:id="@+id/mobile_number_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="Mobile" />


    <TextView
        android:id="@+id/mobile_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_marginStart="75dp"
        android:layout_toEndOf="@+id/mobile_number_title"
        android:text="9940508006" />

    <TextView
        android:id="@+id/websiteTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="130dp"
        android:text="Website" />

    <TextView
        android:id="@+id/website"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="130dp"
        android:layout_marginStart="67dp"
        android:layout_toEndOf="@+id/websiteTitle"
        android:text="www.gowtham.com" />

    <TextView
        android:id="@+id/email_id_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:text="Email" />

    <TextView
        android:id="@+id/email_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:layout_marginStart="83dp"
        android:layout_toEndOf="@+id/email_id_title"
        android:text="gowtham@gmail.com" />

</RelativeLayout>
